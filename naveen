#include "stdafx.h"
#ifndef	_CBrushSealGeometry_HeaderFile_
#include <BrushSealGeometry.h>
#endif //_CBrushSealGeometry_HeaderFile_

//////////////////////////////// PUBLIC ///////////////////////////////////////  
//================================ LIFECYCLE ================================== 
//======================= Construction and Destruction ======================== 
/*******************************************************************************/
//   Function Name  :   Constructor with parameters
//   Parameters     :   const Ptr(CVNet_1_0DocController)& -->DocController Obj
//   Description    :   
//   Modify History :
//      1.              Date    ->  23/08/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/
CBrushSealGeometry::CBrushSealGeometry (const Ptr(HFWDataSource)& l_pDataSource)
				  :CGeometricAttributes(l_pDataSource)  //hint
{
	Raise_If_NULL_SMART_PTR( l_pDataSource,"CBrushSealGeometry::CBrushSealGeometry"); //hint

	Ptr(HFWDataTable) l_pDataTab = l_pDataSource->GetTable("BrushSealGeometry"); //hint
	m_pBSGeomDataRow = l_pDataTab->AddNewRow();

	//Setting the Geometric Attributes ID
	SetGeomAttribID(CGeometricAttributes::GetObjectID());
}
/*******************************************************************************/
//   Function Name  :   Constructor with Parameters
//   Parameters     :   const Ptr(CVNet_1_0DocController)& -> Doc Controller Object
//						const TCollection_AsciiString& - Row ID 
//   Description    :   
//   Modify History :
//      1.              Date    ->  23/08/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/
CBrushSealGeometry::CBrushSealGeometry (const Ptr(HFWDataSource)& l_pDataSource,
									  const TCollection_AsciiString& l_strID)
									  :CGeometricAttributes() //hint
{
	Raise_If_NULL_SMART_PTR( l_pDataSource,"CBrushSealGeometry::CBrushSealGeometry"); //hint
	m_pDataSrc = l_pDataSource->GetDataSource(); //hint
	Ptr(HFWDataTable) l_pDataTab = m_pDataSrc->GetTable("BrushSealGeometry");
	m_pBSGeomDataRow = l_pDataTab->GetRow(l_strID);

	//Geometric Attribute Row
	Ptr(HFWDataTable) l_pGeomDataTab = m_pDataSrc->GetTable("GeometricAttributes");
	m_pDataRow = l_pGeomDataTab->GetRow(m_pBSGeomDataRow->GetColumn("GeometricAttributesObjID")->GetData());
}

/*******************************************************************************/
//   Function Name  :   Destructor
//   Parameters     :   
//   Description    :   
//   Modify History :-
//      1.              Date    ->  23/08/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/
CBrushSealGeometry::~CBrushSealGeometry ()
{
}

/*******************************************************************************/
//   Function Name  :   SetRadius
//   Parameters     :   TCollection_AsciiString&
//   Description    :   Sets the Radius
//   Modify History :
//      1.              Date    ->  23/08/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/
void CBrushSealGeometry::SetRadius(TCollection_AsciiString& pRadius)
{
	m_pBSGeomDataRow->GetColumn("Radius")->SetData(pRadius);
}

/*******************************************************************************/
//   Function Name  :   GetRadius
//   Parameters     :   
//	 Return			:	TCollection_AsciiString
//   Description    :   Returns the Radius
//   Modify History :
//      1.              Date    ->  23/08/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/

TCollection_AsciiString CBrushSealGeometry::GetRadius()
{
	return m_pBSGeomDataRow->GetColumn("Radius")->GetData();
}
/*******************************************************************************/
//   Function Name  :   GetGeomObjID
//   Parameters     :   
//	 Return			:	TCollection_AsciiString
//   Description    :   Returns the Geometry Attributes Object ID
//   Modify History :
//      1.              Date    ->  20/09/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/

TCollection_AsciiString  CBrushSealGeometry::GetGeomObjID()
{
	return m_pBSGeomDataRow->GetColumn("GeometricAttributesObjID")->GetData();
}

/*******************************************************************************/
//   Function Name  :   SetGeomAttribID
//   Parameters     :   TCollection_AsciiString&
//	 Return			:	
//   Description    :   Sets the Geometric Attributes Object ID
//   Modify History :
//      1.              Date    ->  20/09/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/
void  CBrushSealGeometry::SetGeomAttribID(TCollection_AsciiString& pGeomObjID)
{
	m_pBSGeomDataRow->GetColumn("GeometricAttributesObjID")->SetData(pGeomObjID);
}
/*******************************************************************************/
//   Function Name  :   GetObjectID
//   Parameters     :   
//	 Return			:	TCollection_AsciiString 
//   Description    :   Return the Object ID
//   Modify History :
//      1.              Date    ->  28/09/2006 
//                      Author  ->  Vijay Varanasi
//                      Purpose ->  Created
/*******************************************************************************/
TCollection_AsciiString CBrushSealGeometry::GetObjectID()
{
	return m_pBSGeomDataRow->GetRowID();
}
