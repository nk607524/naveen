#include <NCollection_DefineList.hxx>
#endif //NCollection_DefineList_HeaderFile

#ifndef __CGeometricAttributes_HeaderFile__
#include <GeometricAttributes.h>
#endif //__CGeometricAttribute_HeaderFile__

#ifndef __VNet_1_0DocController_HeaderFile__
#include <VNet_1_0DocController.h>
#endif //__VNet_1_0DocController_HeaderFile__


// Class Definition

class CBrushSealGeometry : public CGeometricAttributes{

public:
	CBrushSealGeometry (const Ptr(HFWDataSource)& l_pDataSource); //hint
	CBrushSealGeometry (const Ptr(HFWDataSource)& l_pDataSource,const TCollection_AsciiString& l_strID); //hint
	
	virtual ~CBrushSealGeometry ();

	virtual void SetRadius(TCollection_AsciiString& pRadius);
	void SetGeomAttribID(TCollection_AsciiString& pGeomObjID);
	virtual void SetGeomID(TCollection_AsciiString& l_strID)
	{
	}

	virtual TCollection_AsciiString GetRadius();
	TCollection_AsciiString GetGeomObjID();
	TCollection_AsciiString GetObjectID();

protected:
	CBrushSealGeometry()
	{
	}
	Ptr(HFWDataRow) m_pBSGeomDataRow;
		
private:
};
DEFINE_SMART_PTR(CBrushSealGeometry,CGeometricAttributes)


